lector('.play-path');\r\n      if (playPath) {\r\n        if (podcastBlock.classList.contains('active')) {\r\n          // Якщо відтворення, то змінюємо форму на \"pause\"\r\n          playPath.setAttribute('d', 'M256 512A256 256 0 1 0 256 0a256 256 0 1 0 0 512zM224 192V320c0 17.7-14.3 32-32 32s-32-14.3-32-32V192c0-17.7 14.3-32 32-32s32 14.3 32 32zm128 0V320c0 17.7-14.3 32-32 32s-32-14.3-32-32V192c0-17.7 14.3-32 32-32s32 14.3 32 32z');\r\n        } else {\r\n          // Якщо пауза, то змінюємо форму на \"play\"\r\n          playPath.setAttribute('d', 'M0 256a256 256 0 1 1 512 0A256 256 0 1 1 0 256zM188.3 147.1c-7.6 4.2-12.3 12.3-12.3 20.9V344c0 8.7 4.7 16.7 12.3 20.9s16.8 4.1 24.3-.5l144-88c7.1-4.4 11.5-12.1 11.5-20.5s-4.4-16.1-11.5-20.5l-144-88c-7.4-4.5-16.7-4.7-24.3-.5z');\r\n        }\r\n      }\r\n    },\r\n  },\r\n  async created() {\r\n    this.loading = true;\r\n\r\n    const token = localStorage.getItem('token');\r\n    // отримання даних всіх подкастів\r\n    const responseFavoritePodcast = await apiService.getFavoritePodcasts(token);\r\n\r\n    if (responseFavoritePodcast && responseFavoritePodcast.data) {\r\n      this.podcasts = responseFavoritePodcast.data;\r\n      console.log(this.podcasts);\r\n    }\r\n  }\r\n}\r\n</script>"],"mappings":"AAmDA,OAAOA,UAAS,MAAO,0BAA0B;AAEjD,eAAe;EACbC,IAAI,EAAE,uBAAuB;EAC7BC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,EAAE;MACZC,OAAO,EAAE;IACX;EACF,CAAC;EACDC,OAAO,EAAE;IACP;IACAC,sBAAsBA,CAACC,OAAO,EAAEC,KAAK,EAAE;MACrCD,OAAO,CAACE,UAAS,GAAI,CAACF,OAAO,CAACE,UAAU;MACxC;IACF,CAAC;;IACD;IACAC,qBAAqBA,CAACF,KAAK,EAAE;MAC3B,IAAIG,YAAW,GAAIH,KAAK,CAACI,MAAM,CAACC,OAAO,CAAC,qBAAqB,CAAC;MAC9DF,YAAY,CAACG,SAAS,CAACC,MAAM,CAAC,QAAQ,CAAC;MAEvC,IAAIC,cAAa,GAAIR,KAAK,CAACI,MAAM,CAACC,OAAO,CAAC,UAAU,CAAC;MACrDG,cAAc,CAACF,SAAS,CAACC,MAAM,CAAC,QAAQ,CAAC;MAEzC,MAAME,cAAa,GAAID,cAAc,CAACE,aAAa,CAAC,UAAU,CAAC;MAC/DD,cAAc,CAACH,SAAS,CAACC,MAAM,CAAC,UAAU,CAAC;MAE3C,MAAMI,WAAU,GAAIH,cAAc,CAACE,aAAa,CAAC,cAAc,CAAC;MAChEC,WAAW,CAACL,SAAS,CAA