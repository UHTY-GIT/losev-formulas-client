rdIconClass.value = 'fas fa-eye-slash';\r\n      }\r\n    }\r\n\r\n    return {\r\n      vName$,\r\n      vPassword$,\r\n      submitUserName,\r\n      submitUserPassword,\r\n      OldTogglePassword,\r\n      NewTogglePassword,\r\n      isOldPasswordVisible,\r\n      isNewPasswordVisible,\r\n      OldPasswordIconClass,\r\n      name,\r\n      password,\r\n      newPassword\r\n    }\r\n  },\r\n  validations () {\r\n    return {\r\n      name: { required },\r\n      password: { required },\r\n      newPassword: { required, minLength: minLength(6) }, // нова змінна для валідації нового пароля\r\n    }\r\n  },\r\n  methods: {\r\n    //Виводимо ключ get запитом у url шоб вивнести повідомлення logout на екран на сторінці UserLogin\r\n    logout() {\r\n      this.$router.push('/login?message=logout')\r\n    }\r\n  },\r\n}\r\n</script>"],"mappings":";AAyFA;AACA,SAAQA,GAAG,QAAO,KAAK;AACvB,OAAOC,YAAW,MAAO,iBAAiB;AAC1C,SAAQC,QAAQ,EAAEC,SAAS,QAAO,uBAAuB;AAEzD,eAAe;EACbC,IAAI,EAAE,aAAa;EACnBC,KAAIA,CAAA,EAAK;IACP,MAAMD,IAAG,GAAIJ,GAAG,CAAC,EAAE,CAAC;IACpB,MAAMM,QAAO,GAAIN,GAAG,CAAC,EAAE,CAAC;IACxB,MAAMO,WAAU,GAAIP,GAAG,CAAC,EAAE,CAAC;IAE3B,MAAMQ,MAAK,GAAIP,YAAY,CAAC,CAAC;IAC7B,MAAMQ,UAAS,GAAIR,YAAY,CAAC,CAAC;;IAEjC;IACA,MAAMS,cAAa,GAAI,MAAAA,CAAA,KAAY;MACjC,IAAIF,MAAM,CAACG,KAAK,CAACC,QAAQ,EAAE;QACzBJ,MAAM,CAACG,KAAK,CAACE,MAAM,CAAC,CAAC;QACrBC,OAAO,CAACC,GAAG,CAAC,yBAAyB;QACrC;MACF;MACA,MAAMC,gBAAe,GAAI;QACvBZ,IAAI,EAAEA,IAAI,CAACO;MACb,CAAC;MACDG,OAAO,CAACC,GAAG,CAACC,gBAAgB,CAAC;IAC/B;;IAEA;IACA,MAAMC,kBAAiB,GAAI,MAAAA,CAAA,KAAY;MACrC,IAAIR,UAAU,CAACE,KAAK,CAACC,QAAQ,EAAE;QAC7BH,UAAU,CAACE,KAAK,CAACE,MAAM,CAAC,CAAC;QACzBC,OAAO,CAACC,GAAG,CAAC,6BAA6B;QACzC;MACF;MACA,MAAMG,gBAAe,GAAI;QACvBZ,QAAQ,EAAEA,QAAQ,CAACK,KAAK;QACxBJ,WAAW,EAAEA,WAAW,CAACI;MAC3B,CAAC;MACDG,OAAO,CAACC,GAAG,CAACG,gBAAgB,CAAC;IAC/B;;IAEA;IACA,MAAMC,oBAAmB,GAAInB,GAAG,CAAC,KAAK,CAAC;IAEvC,MAAMoB,oBAAmB,GAAIpB,GAAG,CAAC,kBAAkB,CAAC,EAAE;;IAEtD;IACA,MAAMqB,iBAAgB,GAAIA,CAAA,KAAM;MAC9BF,oBAAoB,CAACR,KAAI,GAAI,CA