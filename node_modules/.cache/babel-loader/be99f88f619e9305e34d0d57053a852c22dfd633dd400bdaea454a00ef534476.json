/');\r\n    }\r\n\r\n    // Реактивний стан для перевірки видимості пароля\r\n    const isPasswordVisible = ref(false);\r\n\r\n    const passwordIconClass = ref('fas fa-eye-slash'); // Додано реактивний об'єкт для зміни іконки\r\n\r\n    // Функція переключення видимості пароля\r\n    const togglePassword = () => {\r\n      isPasswordVisible.value = !isPasswordVisible.value;\r\n      if (isPasswordVisible.value) {\r\n        passwordIconClass.value = 'fas fa-eye';\r\n      } else {\r\n        passwordIconClass.value = 'fas fa-eye-slash';\r\n      }\r\n    }\r\n    return {\r\n      isPasswordVisible,\r\n      togglePassword,\r\n    }\r\n  },\r\n  created() {\r\n    this.backgroundClass = inject('backgroundClass');\r\n  },\r\n}\r\n</script>"],"mappings":";AAoFA,SAASA,GAAE,QAAS,KAAK;AACzB,SAASC,MAAK,QAAS,KAAK;AAC5B,OAAOC,YAAW,MAAO,iBAAgB;AACzC,SAASC,QAAQ,EAAEC,KAAK,EAAEC,SAAQ,QAAS,uBAAsB;AACjE,SAAQC,SAAS,QAAO,YAAY;AAGpC,eAAe;EACbC,IAAI,EAAE,cAAc;EACpBC,IAAGA,CAAA,EAAK;IACN,OAAO;MACLC,eAAe,EAAE,IAAI;MACrBL,KAAK,EAAE,EAAE;MACTM,QAAQ,EAAE;IACZ;EACF,CAAC;EACDC,KAAIA,CAAA,EAAK;IACP,MAAMC,MAAK,GAAIN,SAAS,CAAC,CAAC,EAAE;IAC5B,MAAMO,EAAC,GAAIX,YAAY,CAAC,CAAC,EAAE;IAC3B,MAAMY,aAAY,GAAIA,CAAA,KAAM;MAC1B,IAAID,EAAE,CAACE,KAAK,CAACC,QAAQ,EAAE;QACrBH,EAAE,CAACE,KAAK,CAACE,MAAM,CAAC,CAAC;QACjBC,OAAO,CAACC,GAAG,CAAC,wBAAwB;QACpC;MACF;MAEAP,MAAM,CAACQ,IAAI,CAAC,GAAG,CAAC;IAClB;;IAEA;IACA,MAAMC,iBAAgB,GAAIrB,GAAG,CAAC,KAAK,CAAC;IAEpC,MAAMsB,iBAAgB,GAAItB,GAAG,CAAC,kBAAkB,CAAC,EAAE;;IAEnD;IACA,MAAMuB,cAAa,GAAIA,CAAA,KAAM;MAC3BF,iBAAiB,CAACN,KAAI,GAAI,CAACM,iBAAiB,CAACN,KAAK;MAClD,IAAIM,iBAAiB,CAACN,KAAK,EAAE;QAC3BO,iBAAiB,CAACP,KAAI,GAAI,YAAY;MACxC,OAAO;QACLO,iBAAiB,CAACP,KAAI,GAAI,kBAAkB;MAC9C;IACF;IACA,OAAO;MACLM,iBAAiB;MACjBE;IACF;EACF,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACf,eAAc,GAAIR,MAAM,CAAC,iBAAiB,CAAC;EAClD;AACF"},"metadata":{},"sourceType":"module","externalDependencies":[]}