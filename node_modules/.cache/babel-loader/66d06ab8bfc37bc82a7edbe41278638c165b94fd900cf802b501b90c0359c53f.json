{"ast":null,"code":"import homeIcon from '@/assets/icons/home-1.png';\nimport allPodcastIcon from '@/assets/icons/all-podcast-1.png';\nimport categoriesIcon from '@/assets/icons/categories-1.png';\nexport default {\n  data: () => ({\n    links: [{\n      title: 'Головна',\n      url: '/',\n      icon: homeIcon\n    }, {\n      title: 'Всі подкасти',\n      url: '/allpodcast',\n      exact: true,\n      icon: allPodcastIcon\n    }, {\n      title: 'Категорії',\n      url: '/',\n      exact: true,\n      icon: categoriesIcon\n    }],\n    isSmall: false\n  }),\n  methods: {\n    toggleSize() {\n      this.isSmall = !this.isSmall;\n      //Надіслати подію батькові при перемиканні розміру\n      this.$emit('toggle-size', this.isSmall);\n    }\n  }\n};","map":{"version":3,"names":["homeIcon","allPodcastIcon","categoriesIcon","data","links","title","url","icon","exact","isSmall","methods","toggleSize","$emit"],"sources":["C:\\Users\\Denys\\WebstormProjects\\losev-formulas-client\\src\\components\\app\\SideBar.vue"],"sourcesContent":["<template>\r\n  <nav class=\"sidebar-menu\" :class=\"{ small: isSmall }\">\r\n    <div class=\"menu-header\">\r\n      <a href=\"#\" @click.prevent=\"toggleSize\">\r\n        <img class=\"menu-icon\" src=\"@/assets/icons/menu-1.png\" alt=\"Menu icon\">\r\n        <span v-show=\"!isSmall\">МЕНЮ</span>\r\n      </a>\r\n    </div>\r\n    <ul class=\"menu-ul\">\r\n      <router-link\r\n          v-for=\"link in links\"\r\n          :key=\"link.url\"\r\n          :to=\"link.url\"\r\n          :exact=\"link.exact\"\r\n          active-class=\"active\"\r\n          custom\r\n          v-slot=\"{ navigate, isActive }\"\r\n      >\r\n        <li\r\n            class=\"menu-item\"\r\n            :class=\"{ 'active': isActive }\"\r\n            @click=\"navigate\"\r\n        >\r\n          <a>\r\n            <img class=\"menu-icon\" :src=\"link.icon\" :alt=\"`${link.title} icon`\">\r\n            <span v-show=\"!isSmall\">{{ link.title }}</span>\r\n          </a>\r\n        </li>\r\n      </router-link>\r\n    </ul>\r\n  </nav>\r\n</template>\r\n<script>\r\nimport homeIcon from '@/assets/icons/home-1.png';\r\nimport allPodcastIcon from '@/assets/icons/all-podcast-1.png';\r\nimport categoriesIcon from '@/assets/icons/categories-1.png';\r\n\r\nexport default {\r\n  data: () => ({\r\n    links: [\r\n      {title: 'Головна', url: '/', icon: homeIcon},\r\n      {title: 'Всі подкасти', url: '/allpodcast', exact:true, icon: allPodcastIcon},\r\n      {title: 'Категорії', url: '/', exact:true, icon: categoriesIcon},\r\n    ],\r\n    isSmall: false\r\n  }),\r\n  methods: {\r\n    toggleSize() {\r\n      this.isSmall = !this.isSmall;\r\n      //Надіслати подію батькові при перемиканні розміру\r\n      this.$emit('toggle-size', this.isSmall);\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style>\r\n.sidebar-menu.small {\r\n  width: 70px;\r\n}\r\n.menu-ul {\r\n  display: flex;\r\n  flex-direction: column;\r\n  flex-wrap: nowrap;\r\n}\r\n.menu-header a {\r\n  padding: 15px 0 25px;\r\n  display: flex;\r\n  align-items: center;\r\n  align-content: center;\r\n  flex-wrap: nowrap;\r\n  flex-direction: row;\r\n}\r\n.menu-header a span {\r\n  line-height: 0;\r\n}\r\n</style>"],"mappings":"AAiCA,OAAOA,QAAO,MAAO,2BAA2B;AAChD,OAAOC,cAAa,MAAO,kCAAkC;AAC7D,OAAOC,cAAa,MAAO,iCAAiC;AAE5D,eAAe;EACbC,IAAI,EAAEA,CAAA,MAAO;IACXC,KAAK,EAAE,CACL;MAACC,KAAK,EAAE,SAAS;MAAEC,GAAG,EAAE,GAAG;MAAEC,IAAI,EAAEP;IAAQ,CAAC,EAC5C;MAACK,KAAK,EAAE,cAAc;MAAEC,GAAG,EAAE,aAAa;MAAEE,KAAK,EAAC,IAAI;MAAED,IAAI,EAAEN;IAAc,CAAC,EAC7E;MAACI,KAAK,EAAE,WAAW;MAAEC,GAAG,EAAE,GAAG;MAAEE,KAAK,EAAC,IAAI;MAAED,IAAI,EAAEL;IAAc,CAAC,CACjE;IACDO,OAAO,EAAE;EACX,CAAC,CAAC;EACFC,OAAO,EAAE;IACPC,UAAUA,CAAA,EAAG;MACX,IAAI,CAACF,OAAM,GAAI,CAAC,IAAI,CAACA,OAAO;MAC5B;MACA,IAAI,CAACG,KAAK,CAAC,aAAa,EAAE,IAAI,CAACH,OAAO,CAAC;IACzC;EACF;AACF"},"metadata":{},"sourceType":"module","externalDependencies":[]}