ssword: { required, minLength: minLength(6) } // припустимо, мінімальна довжина пароля - 6 символів\r\n    }\r\n  },\r\n  created() {\r\n    this.backgroundClass = inject('backgroundClass');\r\n  },\r\n}\r\n\r\n</script>\r\n"],"mappings":";AA4GA,SAASA,YAAW,QAAS,iBAAgB;AAC7C,SAASC,QAAQ,EAAEC,KAAK,EAAEC,SAAQ,QAAS,uBAAsB;AACjE,SAAQC,MAAM,QAAO,KAAK;AAC1B,SAAQC,SAAS,QAAO,YAAY;AAEpC,eAAe;EACbC,KAAIA,CAAA,EAAK;IACP,MAAMC,MAAK,GAAIF,SAAS,CAAC,CAAC,EAAE;IAC5B,MAAMG,EAAC,GAAIR,YAAY,CAAC,CAAC,EAAE;IAC3B,MAAMS,aAAY,GAAIA,CAAA,KAAM;MAC1B,IAAID,EAAE,CAACE,KAAK,CAACC,QAAQ,EAAE;QACrBH,EAAE,CAACE,KAAK,CAACE,MAAM,CAAC,CAAC;QACjB;MACF;MACAC,OAAO,CAACC,GAAG,CAAC,wBAAwB;MACpCP,MAAM,CAACQ,IAAI,CAAC,GAAG,CAAC;IAClB;IAEA,OAAO;MACLP,EAAE;MACFC,aAAY,CAAE;IAChB;EACF,CAAC;;EACDO,IAAGA,CAAA,EAAK;IACN,OAAO;MACLC,eAAe,EAAE,IAAI;MACrBf,KAAK,EAAE,EAAE;MACTgB,QAAQ,EAAE;IACZ;EACF,CAAC;EACDC,WAAUA,CAAA,EAAK;IACb,OAAO;MACLjB,KAAK,EAAE;QAAED,QAAQ;QAAEC;MAAM,CAAC;MAC1BgB,QAAQ,EAAE;QAAEjB,QAAQ;QAAEE,SAAS,EAAEA,SAAS,CAAC,CAAC;MAAE,EAAE;IAClD;EACF,CAAC;;EACDiB,OAAOA,CAAA,EAAG;IACR,IAAI,CAACH,eAAc,GAAIb,MAAM,CAAC,iBAAiB,CAAC;EAClD;AACF"},"metadata":{},"sourceType":"module","externalDependencies":[]}