{"ast":null,"code":"import apiService from '@/services/apiService';\nimport defaultUserPhoto from '@/assets/icons/user-photo-1.png';\nexport default {\n  name: 'NavBar',\n  data() {\n    return {\n      username: null,\n      userphoto: defaultUserPhoto,\n      searchQuery: '',\n      // Реактивний стан для зберігання пошукового запиту\n      searchResults: [] // Реактивний стан для зберігання результатів пошуку\n    };\n  },\n\n  methods: {\n    async searchPodcasts() {\n      try {\n        const response = await apiService.searchPodcasts(this.searchQuery);\n        this.searchResults = response.data; // Оновлюємо результати пошуку\n        console.log(this.searchResults.data);\n      } catch (error) {\n        console.error('Під час пошуку сталася помилка: ', error);\n      }\n    }\n  },\n  async mounted() {\n    const token = localStorage.getItem('token');\n    console.log(token);\n    // перевіряємо чи є токен\n    if (token) {\n      const userProfile = await apiService.getUserProfile(token);\n      // Отримуємо дані такі як аватар і ім'я\n      if (userProfile && userProfile.data) {\n        this.username = userProfile.data.name;\n        this.userphoto = userProfile.data.avatar ? userProfile.data.avatar : defaultUserPhoto;\n      }\n    }\n  }\n};","map":{"version":3,"names":["apiService","defaultUserPhoto","name","data","username","userphoto","searchQuery","searchResults","methods","searchPodcasts","response","console","log","error","mounted","token","localStorage","getItem","userProfile","getUserProfile","avatar"],"sources":["C:\\Users\\Denys\\WebstormProjects\\losev-formulas-client\\src\\components\\app\\NavBar.vue"],"sourcesContent":["<template>\r\n  <header>\r\n    <div class=\"header_items\">\r\n      <div>\r\n        <img class=\"logotype\" src=\"@/assets/photo/logo-4-1.png\" alt=\"logotype\">\r\n      </div>\r\n      <div class=\"search\">\r\n        <input type=\"text\" v-model.trim=\"searchQuery\" @input=\"searchPodcasts\" placeholder=\"Пошук\">\r\n        <img class=\"search-icon\" src=\"@/assets/icons/search-1.png\" alt=\"search icon\">\r\n      </div>\r\n      <!-- Відображення результатів пошуку -->\r\n      <div class=\"search-results\" v-if=\"searchResults.length > 0\">\r\n        <ul>\r\n          <li v-for=\"result in searchResults\" :key=\"result.id\">\r\n            {{ result.title }} by {{ result.author }}\r\n          </li>\r\n        </ul>\r\n      </div>\r\n      <div class=\"autorizhate\">\r\n        <router-link to=\"/settings\"><img src=\"@/assets/icons/settings-1.png\" alt=\"settings\"></router-link>\r\n        <router-link v-if=\"username\" to=\"/profile\" class=\"username-account\"><img :src=\"userphoto\" alt=\"userphoto\"> {{ username }}</router-link>\r\n        <router-link v-if=\"!username\" to=\"/login\">Login</router-link>\r\n        <router-link v-if=\"!username\" to=\"/register\">Sign up</router-link>\r\n      </div>\r\n    </div>\r\n  </header>\r\n</template>\r\n\r\n<script>\r\nimport apiService from '@/services/apiService';\r\nimport defaultUserPhoto from '@/assets/icons/user-photo-1.png';\r\n\r\nexport default {\r\n  name: 'NavBar',\r\n  data() {\r\n    return {\r\n      username: null,\r\n      userphoto: defaultUserPhoto,\r\n      searchQuery: '', // Реактивний стан для зберігання пошукового запиту\r\n      searchResults: [], // Реактивний стан для зберігання результатів пошуку\r\n    };\r\n  },\r\n  methods: {\r\n    async searchPodcasts() {\r\n      try {\r\n        const response = await apiService.searchPodcasts(this.searchQuery);\r\n        this.searchResults = response.data; // Оновлюємо результати пошуку\r\n        console.log(this.searchResults.data)\r\n      } catch (error) {\r\n        console.error('Під час пошуку сталася помилка: ', error);\r\n      }\r\n    },\r\n  },\r\n  async mounted() {\r\n    const token = localStorage.getItem('token');\r\n    console.log(token);\r\n    // перевіряємо чи є токен\r\n    if (token) {\r\n      const userProfile = await apiService.getUserProfile(token);\r\n      // Отримуємо дані такі як аватар і ім'я\r\n      if (userProfile && userProfile.data) {\r\n        this.username = userProfile.data.name;\r\n        this.userphoto = userProfile.data.avatar ? userProfile.data.avatar : defaultUserPhoto;\r\n      }\r\n    }\r\n  },\r\n};\r\n</script>\r\n<style>\r\n.search-results {\r\n  display: block;\r\n  height: 200px;\r\n  width: 600px;\r\n  background-color: #FFFFFF;\r\n  color: #000000;\r\n}\r\n</style>"],"mappings":"AA6BA,OAAOA,UAAS,MAAO,uBAAuB;AAC9C,OAAOC,gBAAe,MAAO,iCAAiC;AAE9D,eAAe;EACbC,IAAI,EAAE,QAAQ;EACdC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,IAAI;MACdC,SAAS,EAAEJ,gBAAgB;MAC3BK,WAAW,EAAE,EAAE;MAAE;MACjBC,aAAa,EAAE,EAAE,CAAE;IACrB,CAAC;EACH,CAAC;;EACDC,OAAO,EAAE;IACP,MAAMC,cAAcA,CAAA,EAAG;MACrB,IAAI;QACF,MAAMC,QAAO,GAAI,MAAMV,UAAU,CAACS,cAAc,CAAC,IAAI,CAACH,WAAW,CAAC;QAClE,IAAI,CAACC,aAAY,GAAIG,QAAQ,CAACP,IAAI,EAAE;QACpCQ,OAAO,CAACC,GAAG,CAAC,IAAI,CAACL,aAAa,CAACJ,IAAI;MACrC,EAAE,OAAOU,KAAK,EAAE;QACdF,OAAO,CAACE,KAAK,CAAC,kCAAkC,EAAEA,KAAK,CAAC;MAC1D;IACF;EACF,CAAC;EACD,MAAMC,OAAOA,CAAA,EAAG;IACd,MAAMC,KAAI,GAAIC,YAAY,CAACC,OAAO,CAAC,OAAO,CAAC;IAC3CN,OAAO,CAACC,GAAG,CAACG,KAAK,CAAC;IAClB;IACA,IAAIA,KAAK,EAAE;MACT,MAAMG,WAAU,GAAI,MAAMlB,UAAU,CAACmB,cAAc,CAACJ,KAAK,CAAC;MAC1D;MACA,IAAIG,WAAU,IAAKA,WAAW,CAACf,IAAI,EAAE;QACnC,IAAI,CAACC,QAAO,GAAIc,WAAW,CAACf,IAAI,CAACD,IAAI;QACrC,IAAI,CAACG,SAAQ,GAAIa,WAAW,CAACf,IAAI,CAACiB,MAAK,GAAIF,WAAW,CAACf,IAAI,CAACiB,MAAK,GAAInB,gBAAgB;MACvF;IACF;EACF;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}