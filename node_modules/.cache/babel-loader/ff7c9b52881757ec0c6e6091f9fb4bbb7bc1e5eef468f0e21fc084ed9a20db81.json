\r\n    };\r\n  },\r\n  async created() {\r\n    this.loading = true;\r\n\r\n    const response = await apiService.TopPodcastPage();\r\n    // this.podcasts = response.data ? response.data.data : []; // Перевірка на наявність даних перед присвоєнням TOPpodcasts\r\n    //\r\n    //console.log(response.data.data);\r\n\r\n\r\n    if(response && response.data && response.data.data) {\r\n      console.log(response.data);\r\n      this.podcasts = response.data.data.map(podcast => {\r\n        return {\r\n          ...podcast\r\n        };\r\n      });\r\n    }\r\n\r\n    this.loading = false; // Помічаємо завантаження як завершене\r\n  }\r\n}\r\n</script>"],"mappings":"AAmEA,OAAOA,UAAS,MAAO,0BAA0B;AAEjD,eAAe;EACbC,IAAI,EAAE,UAAU;EAChBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,EAAE;MACZC,OAAO,EAAE;IACX,CAAC;EACH,CAAC;EACD,MAAMC,OAAOA,CAAA,EAAG;IACd,IAAI,CAACD,OAAM,GAAI,IAAI;IAEnB,MAAME,QAAO,GAAI,MAAMN,UAAU,CAACO,cAAc,CAAC,CAAC;IAClD;IACA;IACA;;IAGA,IAAGD,QAAO,IAAKA,QAAQ,CAACJ,IAAG,IAAKI,QAAQ,CAACJ,IAAI,CAACA,IAAI,EAAE;MAClDM,OAAO,CAACC,GAAG,CAACH,QAAQ,CAACJ,IAAI,CAAC;MAC1B,IAAI,CAACC,QAAO,GAAIG,QAAQ,CAACJ,IAAI,CAACA,IAAI,CAACQ,GAAG,CAACC,OAAM,IAAK;QAChD,OAAO;UACL,GAAGA;QACL,CAAC;MACH,CAAC,CAAC;IACJ;IAEA,IAAI,CAACP,OAAM,GAAI,KAAK,EAAE;EACxB;AACF"},"metadata":{},"sourceType":"module","externalDependencies":[]}